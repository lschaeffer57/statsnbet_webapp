{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/[^.]+",
      "dest": "/"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/check-expired",
      "schedule": "0 0 * * *"
    }
  ]
}